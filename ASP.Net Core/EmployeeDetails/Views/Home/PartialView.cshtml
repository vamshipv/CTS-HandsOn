<div class="row">
    @foreach (var item in this.Model)
    {
        @if (item.IsPermanent)
        {
            <div class="col-md-3">
                <ul>
                    <li><b>@item.Name</b></li>
                    <li><b>@item.Salary</b></li>
                </ul>
            </div> }
        else
        {
            <br>
            <div class="col-md-3">
                <ul>
                    <li>@item.Name</li>
                    <li>@item.Salary</li>
                </ul>
            </div>}
    }
</div>